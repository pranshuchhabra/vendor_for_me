                <div class="text">
                  <h2>Personal Information</h2>
                  <!-- <p>Enter your personal information to get closer to copanies.</p> -->
                </div>
                <div class="input-text">
                  <div class="input-div">
                    <input type="text" required require id="user_name">
                    <span>First Name</span>
                  </div>
                  <div class="input-div">
                    <input type="text" required>
                    <span>Last Name</span>
                  </div>
                </div>
                <div class="input-text">
                  <div class="input-div">
                    <input type="text" required require>
                    <span>Company Name</span>
                  </div>

                </div>
                <div class="input-text">
                  <div class="input-div">
                    <select>
                      <option>Select Category</option>
                      <option>Contract</option>
                      <option>Lease</option>
                      <option>Supplier</option>

                    </select>

                  </div>
                  <div class="input-div">

                    <select>
                      <option>Select Type</option>
                      <option>Individual</option>
                      <option>Company</option>

                    </select>
                  </div>
                </div>
                <div class="buttons">
                  <button class="next_button">Next Step</button>
                </div>
              </div>

              <div class="main">
                <!-- <small><i class="fa fa-smile-o"></i></small> -->
                <div class="text">
                  <h2>Classification</h2>
                  <!-- <p>Inform companies about your education life.</p> -->
                </div>
                <div class="input-text">
                  <div class="input-div">
                    <select>
                      <option>Select Subsidiary</option>
                      <option>India</option>
                      <option>USA</option>

                    </select>
                  </div>
                  <div class="input-div">
                    <select>
                      <option>Select Terms</option>
                      <option>NET30</option>
                      <option>NET60</option>
                      <option>NET90</option>

                    </select>
                  </div>
                </div>

                <div class="buttons button_space">
                  <button class="back_button">Back</button>
                  <button class="next_button">Next Step</button>
                </div>
              </div>



              <div class="main">
                <!-- <small><i class="fa fa-smile-o"></i></small> -->
                <div class="text">
                  <h2>Contact Information</h2>
                  <!-- <p>Can you talk about your past work experience?</p> -->
                </div>
                <div class="input-text">
                  <div class="input-div">
                    <input type="text" required require>
                    <span>Email</span>
                  </div>
                  <div class="input-div">
                    <input type="text" required require>
                    <span>Contact</span>
                  </div>

                </div>
                <div class="buttons button_space">
                  <button class="back_button">Back</button>
                  <button class="next_button">Next Step</button>
                </div>

              </div>




